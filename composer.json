{
  "name": "mckltech/thanks-io-php",
  "description": "Thanks.io PHP SDK built on top of Guzzle 6",
  "keywords": [
    "api",
    "guzzle",
    "thanks.io",
    "postcards"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Highways.io",
      "homepage": "https://www.highways.io"
    }
  ],
  "support": {
    "issues": "https://github.com/MCKLtech/thanks-io-php/issues",
    "source": "https://github.com/MCKLtech/thanks-io-php"
  },
  "autoload": {
    "psr-4": {
      "ThanksIO\\": [
        "src"
      ]
    }
  },
  "require": {
    "php": ">= 7.1",
    "ext-json": "*",
    "php-http/client-common": "^1.9 || ^2.0",
    "php-http/client-implementation": "*",
    "php-http/discovery": "^1.4",
    "php-http/httplug": "^1.0 || ^2.0",
    "php-http/message": "^1.7",
    "psr/http-message": "^1.0",
    "php-http/guzzle6-adapter": "^1.0 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "config": {
    "sort-packages": true
  }
}
